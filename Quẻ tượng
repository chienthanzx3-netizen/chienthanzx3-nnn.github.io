<div class="inputs">
    <p>Nhập từ dưới lên (hào 1 → hào 6)</p>
    <input id="l1">
    <input id="l2">
    <input id="l3">
    <input id="l4">
    <input id="l5">
    <input id="l6">
</div>

<button onclick="luanQue()">XEM QUẺ</button>

<div class="result" id="result"></div>

<script>

function luanQue(){

    let vals=[
        l1.value,l2.value,l3.value,
        l4.value,l5.value,l6.value
    ];

    let lines=[];
    let moving=[];

    for(let i=0;i<6;i++){

        let v=parseInt(vals[i]);

        if(![6,7,8,9].includes(v)){
            result.innerHTML="Vui lòng nhập số 6,7,8,9.";
            return;
        }

        if(v==6){ lines.push("0"); moving.push(i); }
        if(v==7){ lines.push("1"); }
        if(v==8){ lines.push("0"); }
        if(v==9){ lines.push("1"); moving.push(i); }
    }

    let hex=lines.slice().reverse().join("");

    let data=hexagrams[hex];

    if(!data){
        result.innerHTML="Không tìm thấy quẻ.";
        return;
    }

    let html=`
    <h2>${data.name}</h2>
    <div><b>Hán tự:</b> ${data.han}</div>
    <div><b>Thoán:</b> ${data.thoan}</div>
    <div><b>Tượng:</b> ${data.tuong}</div>
    <hr>
    <div><b>Đại ý cổ:</b> ${data.co}</div>
    <hr>
    <div><b>Tổng quát:</b> ${data.general}</div>
    <div><b>Công việc:</b> ${data.work}</div>
    <div><b>Tài lộc:</b> ${data.money}</div>
    <div><b>Tình cảm:</b> ${data.love}</div>
    <div><b>Sức khỏe:</b> ${data.health}</div>
    `;

    if(moving.length>0){

        let changed=[...lines];

        moving.forEach(i=>{
            changed[i]=changed[i]=="1"?"0":"1";
        });

        let changedHex=changed.slice().reverse().join("");
        let changedData=hexagrams[changedHex];

        if(changedData){
            html+=`
            <hr>
            <h3>Quẻ biến: ${changedData.name}</h3>
            <div><b>Ý nghĩa chuyển hóa:</b> ${changedData.general}</div>
            `;
        }
    }

    result.innerHTML=html;
}

/* ===== DÁN OBJECT 64 QUẺ CỦA BẠN VÀO ĐÂY ===== */

